double A[N][N];
int gauss(int n){
	rep(cur,1,n){
		int target = -1;
		rep(i,cur,n) if( dcmp(A[i][cur])!=0 ){
			target = i; break;
		}
		if( target==-1 ) return -1;
		else if( target!=cur ) swap(A[target],A[cur]);
		double x = A[cur][cur];
		rep(j,cur,n+1) A[cur][j]/=x;
		rep(i,1,n) if( cur!=i ){
			x= A[i][cur];
			if( dcmp(x)==0 ) continue;
			rep(j,cur,n+1){
				A[i][j]-= A[cur][j]*x;
			}
		}
	}
	return 0;
}
