int fa[N];
int pos[N],tot_seq,depth[N],seq[N*2];
void dfs0(int u){
	seq[pos[u]=++tot_seq] = u;
	for(auto e:G[u]){
		int v = e.to;
		if( v==fa[u] ) continue;
		fa[v] = u;
		depth[v] = depth[u]+1;
		dfs0(v);
		seq[++tot_seq] = u;
	}
}
int LCA(int x,int y){
	int px = pos[x], py = pos[y];
	return st.query(min(px,py),max(px,py));
}
void init_LCA(){
	dfs0(1);
	st.init(tot_seq,seq);
}
