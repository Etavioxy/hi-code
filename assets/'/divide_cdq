int ans[N];
struct sth{ int x,y; int id; };
sth d[N];
struct cmp_x{
	bool operator()(const sth&a,const sth&b){
		if( a.x!=b.x ) return a.x < b.x;
		return a.y < b.y;
	}
};
void solve(int l,int r){
	if( l==r ) return;
	int mid=(l+r)/2;
	solve(l,mid);
	solve(mid+1,r);
	sort(d+l,d+mid+1,cmp_x());
	sort(d+mid+1,d+r+1,cmp_x());
	int j = l;
	rep(i,mid+1,r){
		while( j<=mid && d[j].x <= d[i].x ) bit.add(d[j].y,1),j++;
		ans[d[i].id] += bit.query(d[i].y);
	}
	bit.clear();
}
