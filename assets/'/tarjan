int col[N],tot_col;
int low[N],dfn[N],tim;
void Tarjan(int u){
	static int stk[N],top=0;
	static bool exist[N];
	dfn[u] = low[u] = ++tim;
	stk[++top] = u;
	exist[u] = 1;
	each(i,u){
		int v = bow[i].to;
		if( dfn[v]==0 ){
			Tarjan(v);
			low[u]=min(low[u],low[v]);
		}
		else if( exist[v] ) low[u]=min(low[u],dfn[v]);
	}
	if( dfn[u]==low[u] ){
		int x;
		tot_col++;
		do{
			x = stk[top--];
			exist[x] = 0;
			col[x] = tot_col;
		}while( x!=u );
	}
}
