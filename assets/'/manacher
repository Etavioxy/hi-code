int hw[N*2];
void manacher(int n0,char*s0){
	static char s[N*2];
	int n=1; s[0]=s[1]='#';
	rep(i,1,n0) s[++n]=s0[i], s[++n]='#';
	s[n+1]=0;
	int mid=0, mxr=0;
	rep(i,1,n){
		hw[i] = i<=mxr ? min(mxr-i+1,hw[mid*2-i]):1 ;
		while( s[i-hw[i]]==s[i+hw[i]] ) hw[i]++; hw[i]--;
		if( chk_max(mxr,i+hw[i]-1) ) mid=i;
	}
}
