ll sum[N],f[N];
#define X(i) (sum[i])
#define Y(i) (f[i]-sum[i]*sum[i])
#define slope(j,i) (double(Y(i)-Y(j))/(X(i)-X(j)))
#define dp(j,i) (sum[i]*sum[j]+Y(j))
void solve(int n){
	int L=0;
	vector<int> v;
	v.push_back(0);
	rep(i,1,n){
		int R = v.size()-1;
		while( L<R && dp(v[L+1],i) >= dp(v[L],i) ) L++;
		f[i] = dp(v[L],i);
		while( L<R && slope(v[R],i) > slope(v[R-1],i) ) R--, v.pop_back();
		v.push_back(i);
	}
}
